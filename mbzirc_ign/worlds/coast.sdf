        https://fuel.ignitionrobotics.org/1.0/OpenRobotics/models/Large Crate
      </uri>
      <plugin filename="ignition-gazebo-detachable-joint-system" name="ignition::gazebo::systems::DetachableJoint">
       <parent_link>link</parent_link>
       <child_model>large_crate_g</child_model>
       <child_link>link</child_link>
       <topic>/Vessel_G/detach</topic>
      </plugin>
    </include>

    <model name="max_bounds">
      <static>true</static>
      <link name="link">
        <pose>0 0 0 0 0 0</pose>
        <collision name="x">
          <pose>3000 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1 6000 1000</size>
            </box>
          </geometry>
        </collision>
        <collision name="y">
          <pose>0 3000 0 0 0 0</pose>
          <geometry>
            <box>
              <size>6000 1 1000</size>
            </box>
          </geometry>
        </collision>
        <collision name="-x">
          <pose>-3000 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1 6000 1000</size>
            </box>
          </geometry>
        </collision>
        <collision name="-y">
          <pose>0 -3000 0 0 0 0</pose>
          <geometry>
            <box>
              <size>6000 1 1000</size>
            </box>
          </geometry>
        </collision>
        <collision name="base">
          <pose>0 0 -100 0 0 0</pose>
          <geometry>
            <box>
              <size>6000 6000 1</size>
            </box>
          </geometry>
        </collision>
        <collision name="ceiling">
          <pose>0 0 500 0 0 0</pose>
          <geometry>
            <box>
              <size>6000 6000 1</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

    <model name="base_station">
      <static>true</static>
      <pose>-1501.25 -40 3.64 0 0 0</pose>
      <link name="box_link">
        <collision name="box_collision">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
        </collision>

        <visual name="box_visual">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.2 0.1 1</ambient>
            <diffuse>0.7 0.2 0.1 1</diffuse>
            <specular>0.7 0.2 0.1 1</specular>
          </material>
        </visual>
      </link>

      <plugin
        filename="ignition-gazebo-comms-endpoint-system"
        name="ignition::gazebo::systems::CommsEndpoint">
        <address>base_station/target</address>
        <topic>base_station/target</topic>
      </plugin>
      <plugin
        filename="ignition-gazebo-comms-endpoint-system"
        name="ignition::gazebo::systems::CommsEndpoint">
        <address>base_station/video</address>
        <topic>base_station/video</topic>
      </plugin>
      <plugin filename="libBaseStation.so"
              name="mbzirc::BaseStation">
        <target_topic>base_station/target</target_topic>
        <video_topic>base_station/video</video_topic>
      </plugin>
    </model>

    <wind>
      <linear_velocity>0.3 0 0</linear_velocity>
    </wind>

    <!-- Load the plugin for the wind -->
    <plugin
      filename="ignition-gazebo-wind-effects-system"
      name="ignition::gazebo::systems::WindEffects">
      <force_approximation_scaling_factor>0.002</force_approximation_scaling_factor>
      <horizontal>
        <magnitude>
          <time_for_rise>10</time_for_rise>
          <sin>
            <amplitude_percent>0.05</amplitude_percent>
            <period>60</period>
          </sin>
          <noise type="gaussian">
           <mean>0</mean>
           <stddev>0.0002</stddev>
          </noise>
        </magnitude>
        <direction>
          <time_for_rise>30</time_for_rise>
          <sin>
            <amplitude>5</amplitude>
            <period>20</period>
          </sin>
          <noise type="gaussian">
           <mean>0</mean>
           <stddev>0.03</stddev>
          </noise>
        </direction>
      </horizontal>
      <vertical>
        <noise type="gaussian">
         <mean>0</mean>
         <stddev>0.03</stddev>
        </noise>
      </vertical>
    </plugin>

    <!-- The MBZIRC competition logic plugin -->
    <plugin filename="libGameLogicPlugin.so"
            name="mbzirc::GameLogicPlugin">
      <run_duration_seconds>3600</run_duration_seconds>
      <setup_duration_seconds>600</setup_duration_seconds>
      <logging>
        <!-- Use the <path> element to control where to record the log file.
             The HOME path is used by default -->
        <path>/tmp/ign/mbzirc/logs</path>
      </logging>
      <!-- competition area -->
      <!-- w x h = 10km2
           max flight height is 400 ft / 121.92m
           allow 100m below water level -->
      <geofence>
        <center>0 0 10.96</center>
        <size>3162.28 3162.28 221.92</size>
      </geofence>
      <target>
        <vessel>Vessel B</vessel>
        <small_object>small_case_b</small_object>
        <small_object>small_case_b_duplicate</small_object>
        <large_object>large_grey_box_b</large_object>
        <large_object>large_grey_box_b_duplicate</large_object>
      </target>
    </plugin>

    <!-- uncomment to see visualization of the competition area -->
    <!--
    <model name="competition_area">
      <static>true</static>
      <link name="link">
        <pose>0 0 0 0 0 0</pose>
        <visual name="visual">
          <pose>0 0 10.96 0 0 0</pose>
          <geometry>
            <box>
              <size>3162.28 3162.28 221.92</size>
            </box>
          </geometry>
          <material>
            <diffuse>0 1 0 0.5</diffuse>
          </material>
        </visual>
      </link>
    </model>
    -->

    <model name="ocean_entity_detector">
      <static>true</static>
      <!-- This plugin detects when target objects are dropped into the ocean
           at the defined region. A message is published on the <topic> when
           these events occur -->
      <plugin filename="libEntityDetector.so"
              name="mbzirc::EntityDetector">
        <topic>/mbzirc/target_object_detector/dropped</topic>
        <pose>0 0 -100 0 0 0</pose>
        <geometry>
          <box>
            <size>3162.28 3162.28 10</size>
          </box>
        </geometry>
      </plugin>
    </model>

  </world>
</sdf>
